@model List<GSID.Model.MongodbModels.Permission>
@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            <li class="jstree-open" id="@item.Id"> @item.Name @Html.Action("Children", Request.RequestContext.RouteData.Values["controller"].ToString(), new { ParentID = @item.Id }) </li>
        }
    </ul>
}